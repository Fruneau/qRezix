#!/bin/sh

installQtLib()
{
	install_name_tool -change $3/lib/$1.framework/Versions/4/$1 "@executable_path/../Frameworks/$1.framework/Versions/4/$1" $2
	install_name_tool -change $3/lib/$1.framework/Versions/Current/$1 "@executable_path/../Frameworks/$1.framework/Versions/Current/$1" $2
}

installQtLib QtCore $1 $2
installQtLib QtGui $1 $2
installQtLib QtNetwork $1 $2
